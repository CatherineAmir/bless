<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">


     <record id="coupons_sequence_code" model="ir.sequence">
        <field name="name">Coupons</field>
        <field name="code">bless.giving_coupons</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="prefix">%(year)s/%(month)s/coupons/</field>
        <field name="number_increment">1</field>
         <field name="implementation">no_gap</field>
    </record>

    <record id="hand_giving_sequence_code" model="ir.sequence">
        <field name="name">Hand Giving</field>
        <field name="code">bless.giving_hand_giving</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="prefix">%(year)s/%(month)s/hand_giving/</field>
        <field name="number_increment">1</field>
<!--        <field name="implementation">no_gap</field>-->
    </record>

    <record id="money_giving_sequence_code" model="ir.sequence">
        <field name="name">Money Giving</field>
        <field name="code">bless.giving_money_giving</field>
        <field name="padding">4</field>
        <field name="number_next">1</field>
        <field name="prefix">%(year)s/%(month)s/money_giving/</field>
        <field name="number_increment">1</field>
<!--        <field name="implementation">no_gap</field>-->
    </record>

    </data>
</odoo>